{
  "compilerOptions": {
    "lib": ["es5", "es6"],
    "target": "ES5",
    "module": "commonjs",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "outDir": "./build",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "types": ["node"],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx"
  }
}
